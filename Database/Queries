--4
SELECT empName, COUNT(skillName) AS NumberOfSkills 
FROM MechanicSkill  me 
INNER JOIN Employee e 
ON me.empID = e.empID
GROUP BY e.empID HAVING COUNT(skillName) >= 3;
----------------------------------------------------------------------------------

