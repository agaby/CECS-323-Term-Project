--15
SELECT empName AS 'Mechanic and Advisor' 
FROM Employee 
NATURAL JOIN Mechanic 
NATURAL JOIN ServiceAdvisor 
WHERE Mechanic.empID=ServiceAdvisor.empID;

--16

--17
SELECT itemName 
FROM Item
WHERE packageName IS NULL;

